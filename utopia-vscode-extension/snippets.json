{
  "registerComponent": {
    "prefix": "registerComponent",
    "body": [
      "registerComponent({",
      "  name: ${1}, // name is what appears in the insertion list",
      "  moduleName: ${2}, // name of module, or relative file path",
      "  controls: ${3}, // property controls",
      "  insert: ${4}, ",
      "  requiredImports: ${5}",
      "})"
    ],
    "description": "Register Component"
  }
}
