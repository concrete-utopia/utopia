{
  "registerModule": {
    "prefix": "registerModule",
    "body": ["registerModule"],
    "description": "Register Module Import"
  },
  "registerModule(...)": {
    "prefix": "registerModule(...)",
    "body": [
      "registerModule(",
      "  '${1:${TM_FILEPATH/~(.*)\\..+$/$1/}}', // module name or absolute file path",
      "  {",
      "    ${2:ComponentName}: {",
      "      properties: { // property controls",
      "        ${3:PropName}: {",
      "          control: 'string-input',",
      "          label: '${3:PropName}'",
      "        },",
      "      },",
      "      variants: [ // each option appears in the insert menu",
      "        {",
      "          label: '${2:ComponentName}', // label to use in the insert menu, optional",
      "          code: '<${2:ComponentName} />',",
      "          // additionalImports: `import '/src/styles.css';`, // optional",
      "        },",
      "      ],",
      "    },",
      "  }",
      ")"
    ],
    "description": "Register Module Function Body"
  },
  "registerModule:string property control": {
    "prefix": "registerModule:String Control",
    "body": [
      "${1:PropName}: {",
      "  control: 'string-input',",
