  "name": "utopia",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "description": "The Development Version of Utopia",
  "homepage": "https://utopia.app",
  "bugs": "https://github.com/concrete-utopia/utopia/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/concrete-utopia/utopia"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "check": "tsc --noEmit && pnpm run lint && pnpm run check-dependency-cruiser && pnpm run prettier-check && pnpm run test && pnpm run test-browser",
    "check-ci": "tsc --noEmit && pnpm run lint && pnpm run check-dependency-cruiser && pnpm run prettier-check && pnpm run test-ci && pnpm run test-browser",
    "check-code": "tsc --version && tsc --noEmit && pnpm run lint && pnpm run check-dependency-cruiser && pnpm run prettier-check",
    "lint-other-projects": "eslint --quiet --ignore-path ./.eslintignore --config ../eslint-config-utopia/index.js --no-eslintrc --ext .ts --ext .tsx ../utopia-api/src ../utopia-vscode-extension/src ../utopia-vscode-common/src ../puppeteer-tests/src ../utopia-remix/app",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --quiet && pnpm run lint-other-projects",
    "lint-loud": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint-lite": "eslint \"src/**/*.{js,ts,tsx}\" --quiet --config ../eslint-config-utopia/index.js --no-eslintrc && pnpm run lint-other-projects",
    "check-dependency-cruiser": "depcruise --config .dependency-cruiser.js src",
    "benchmark": "pnpm run build-benchmark && node dist/benchmark.js",
    "test-base": "jest --config ./jest.config.js",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --max_old_space_size=1024\" pnpm run test-base --maxWorkers '50%'",
    "test-ci": "NODE_OPTIONS=\"$NODE_OPENSSL_OPTION --max_old_space_size=3072\" xvfb-run -a pnpm run test-base --maxWorkers=2",
    "test-debug": "NODE_OPTIONS=\"--inspect-brk $NODE_OPENSSL_OPTION\" jest --config ./jest.config.js --runInBand",
    "test-watch": "pnpm run test -- --watch",
    "test-update-snapshot": "pnpm run test -- --updateSnapshot",
    "test-browser-base": "NODE_OPTIONS=$NODE_OPENSSL_OPTION REACT_APP_AUTH0_CLIENT_ID='test' REACT_APP_AUTH0_ENDPOINT='test' REACT_APP_ENVIRONMENT_CONFIG=development karma start",
    "test-browser": "pnpm run test-browser-base karma-no-shard.conf.js --single-run",
    "test-browser-hydrogen": "pnpm run test-browser-base karma-hydrogen.conf.js --single-run",
    "test-browser-full-output": "pnpm run test-browser-base karma-no-shard.conf.js --single-run --full-output",
    "test-browser-debug": "REACT_APP_BROWSER_TEST_DEBUG='true' pnpm run test-browser-base karma-no-shard.conf.js --debug",
    "test-browser-shard-1": "pnpm run test-browser-base karma-shard-1.conf.js --single-run",
    "test-browser-shard-2": "pnpm run test-browser-base karma-shard-2.conf.js --single-run",
    "move-fast-and-break-things": "REACT_APP_ENVIRONMENT_CONFIG=development REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=`git rev-parse HEAD` NODE_OPTIONS=\"--max_old_space_size=8192 $NODE_OPENSSL_OPTION\" webpack-dev-server --progress",
    "build-benchmark": "REACT_APP_ENVIRONMENT_CONFIG=development REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=`git rev-parse HEAD` NODE_OPTIONS=\"--max_old_space_size=8192 $NODE_OPENSSL_OPTION\" webpack --config webpack-benchmarks.config.js --mode development",
    "performance-test": "REACT_APP_ENVIRONMENT_CONFIG=development REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=`git rev-parse HEAD` NODE_OPTIONS=\"--max_old_space_size=8192 $NODE_OPENSSL_OPTION\" PERFORMANCE='true' webpack-dev-server --progress",
    "move-fast-and-break-things-hot": "REACT_APP_ENVIRONMENT_CONFIG=development REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=`git rev-parse HEAD` NODE_OPTIONS=\"--max_old_space_size=8192 $NODE_OPENSSL_OPTION\" HOT='true' webpack-dev-server --progress",
    "production": "REACT_APP_ENVIRONMENT_CONFIG=production REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=$UTOPIA_SHA WEBPACK_MODE='production' NODE_OPTIONS=\"--max_old_space_size=6656 $NODE_OPENSSL_OPTION\" webpack",
    "production-print-json": "pnpm run production -- --json=lib/production-stats.json",
    "staging": "REACT_APP_ENVIRONMENT_CONFIG=staging REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=$UTOPIA_SHA WEBPACK_MODE='staging' NODE_OPTIONS=\"--max_old_space_size=6656 $NODE_OPENSSL_OPTION\" webpack",
    "staging-print-json": "pnpm run staging --  --json=lib/staging-stats.json",
    "branches": "REACT_APP_ENVIRONMENT_CONFIG=branches REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=$UTOPIA_SHA WEBPACK_MODE='branches' NODE_OPTIONS=\"--max_old_space_size=6656 $NODE_OPENSSL_OPTION\" webpack",
    "branches-print-json": "pnpm run branches --  --json=lib/branches-stats.json",
    "preinstall": "npx only-allow pnpm",
    "prettier-check": "prettier --ignore-path ../.prettierignore --config ../prettier.config.js --check src/** ../utopia-api/src/**/* ../utopia-vscode-common/src/** ../utopia-vscode-extension/src/** ../puppeteer-tests/src/** ../website-next/pages/** ../website-next/components/** ../utopia-remix/app/**",
    "prettier-fix": "prettier --ignore-path ../.prettierignore --config ../prettier.config.js --write src/** ../utopia-api/src/**/* ../utopia-vscode-common/src/** ../utopia-vscode-extension/src/** ../puppeteer-tests/src/** ../website-next/pages/** ../website-next/components/** ../utopia-remix/app/**",
    "parse-print-analysis": "npx -s sh ts-node --require ignore-styles --transpile-only ./src/scripts/parse-print-analysis.ts",
    "parse-debug": "npx -s sh ts-node --pretty --require ignore-styles --transpile-only ./src/scripts/parse-debug.ts",
    "fetch-tailwind-classes": "npx -s sh ts-node --pretty --require ignore-styles --transpile-only ./src/scripts/fetch-and-parse-tailwind-classes.ts && npx prettier --write ./src/core/third-party/tailwind-defaults.ts",
    "lint-editor": "lint-staged",
    "dev-fast": "APP_ENV=development REACT_APP_ENVIRONMENT_CONFIG=development REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID REACT_APP_AUTH0_ENDPOINT=$AUTH0_ENDPOINT REACT_APP_COMMIT_HASH=`git rev-parse HEAD` vite --force",
    "vite-build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite build -- -DEBUG",
    "watch-tsc": "tsc --watch --noEmit && NODE_OPTIONS=\"--max_old_space_size=4096 $NODE_OPENSSL_OPTION\"",
    "unused-exports": "ts-unused-exports ./tsconfig.json --ignoreLocallyUsed",
    "eslint-custom-rules-test": "eslint-plugin-local test"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --quiet --ignore-path .eslintignore --config ../eslint-config-utopia/index.js --no-eslintrc",
      "git add"
