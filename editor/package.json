    "../utopia-vscode-extension/src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --quiet --ignore-path .eslintignore --config ../eslint-config-utopia/index.js --no-eslintrc",
      "git add"
    ],
    "../utopia-vscode-extension/src/**/*.{css,html}": [
      "prettier --write",
      "git add"
    ],
    "../utopia-vscode-common/src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --quiet --ignore-path .eslintignore --config ../eslint-config-utopia/index.js --no-eslintrc",
      "git add"
    ],
    "../utopia-vscode-common/src/**/*.{css,html}": [
      "prettier --write",
      "git add"
    ],
    "../website-next/pages/**/*.{js,ts,tsx}": [
      "prettier --write",
