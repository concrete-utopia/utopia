      "settings": {
        "foreground": "#939393"
      }
    },
    {
      "scope": ["keyword.operator.combinator.css"],
      "settings": {
        "foreground": "#D9DBDF"
      }
    },
    {
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#058B00"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#8000D7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "foreground": "#75BFFF",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#86DE74"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#CC3D3D"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
