{
  "name": "Better Solarized Light",
  "tokenColors": [
    {
      "settings": {
        "activeGuide": "#00000055",
        "background": "#fdf6e3",
        "foreground": "#657b83",
        "gutter": "#fdf6e3",
        "popupCss": "\nhtml {\n  background-color: #ebe5d3;\n  color: #657b83;\n}\n\na {\n  color: #268bd2;\n}\n\n.error,\n.deleted {\n  color: #dc322f;\n}\n\n.success,\n.inserted,\n.name {\n  color: #859900;\n}\n\n.warning,\n.modified {\n  color: #b58900;\n}\n\n.type {\n  color: #2aa198;\n  font-style: italic;\n}\n\n.param {\n  color: #cb4b16;\n}\n\n.current {\n  text-decoration: underline;\n}\n",
        "selectionBorder": "#00000016",
        "stackGuide": "#00000040"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93a1a1"
      }
    },

