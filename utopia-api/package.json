{
  "name": "utopia-api",
  "version": "0.4.2",
  "description": "Utopia API",
  "main": "src/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist/",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc --watch",
    "dts-bundle": "npm-dts generate && node update-dts.js index.d.ts ../editor/src/core/es-modules/package-manager/utopia-api-typings.ts",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/concrete-utopia/utopia"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "testRegex": "src/.*\\.spec\\.(jsx?|tsx?)$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "",
  "license": "",
  "homepage": "https://utopia.app",
  "peerDependencies": {
    "react": "17.0.0-rc.1"
  },
  "dependencies": {
    "@emotion/react": "11.1.2",
    "@emotion/serialize": "1.0.0",
    "@emotion/styled": "11.0.0",
    "@emotion/styled-base": "11.0.0",
    "draft-js": "git://github.com/concrete-utopia/draft-js.git#6b21131",
    "draft-js-custom-styles": "2.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@types/draft-js": "0.10.19",
    "@types/jest": "26.0.10",
    "@types/prettier": "^1.18.4",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/uuid": "3.4.5",
    "csstype": "3.0.3",
    "jest": "27.0.6",
    "jest-environment-jsdom": "27.0.6",
    "lint-staged": "^9.4.3",
    "npm-dts": "1.3.1",
    "patch-package": "6.1.2",
    "prettier": "2.0.5",
    "react": "17.0.0-rc.1",
    "react-dom": "17.0.0-rc.1",
    "typescript": "4.0.5"
  }
}
