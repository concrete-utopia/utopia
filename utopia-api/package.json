{
  "name": "utopia-api",
  "version": "0.5.0",
  "description": "Utopia API",
  "main": "src/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./core": "./src/core.ts"
  },
  "typesVersions": {
    "*": {
      "core": [
        "dist/core.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/concrete-utopia/utopia"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "testRegex": "src/.*\\.spec\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "",
  "license": "",
  "homepage": "https://utopia.app",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "29.5.2",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.2.5",
    "typescript": "5.2.2"
  }
}
